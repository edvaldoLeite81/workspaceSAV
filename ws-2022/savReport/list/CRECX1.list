    initialize print-buf to default.                             
    move '   <!-- end html page header -->' to print-buf         
    write printf-r from print-buf after advancing 1 line         
    initialize print-buf to default.                             
is-report-1-page-footer.                                         
    compute tmp-pixels = cnt-inpagepixels + current-height       
    if tmp-pixels > is-report-1-page-height                      
       compute cnt-pixels = (is-pagenum-sys *                    
       is-report-1-page-height)                                  
       compute cnt-inpagepixels = is-report-1-page-height        
       perform is-report-1-page-break-rtn                        
    end-if.                                                      
is-report-1-detail.                                              
    perform is-DT-campo-1-detail-print.                          
is-report-1-do-print-rtn.                                        
    move 1 to tmp-printornot.                                    
    move is-DT-campo-1-height to current-height                  
    perform is-report-1-page-footer.                             
    perform is-report-1-detail.                                  
is-report-1-page-break-rtn.                                      
    if current-col < ntopcolumnnum                               
       compute cnt-pixels = (is-pagenum-sys - 1) *               
       is-report-1-page-height                                   
       compute is-report-1-cleftoffset = is-report-1-cleftoffset 
       + detail-width                                            
       move 0 to cnt-inpagepixels                                
       add 1 to current-col                                      
    else                                                         
       compute cnt-pixels = is-pagenum-sys *                     
       is-report-1-page-height                                   
       move 0 to is-report-1-cleftoffset                         
       move 0 to cnt-inpagepixels                                
       add 1 to is-pagenum-sys                                   
       move 1 to current-col                                     
    end-if                                                       
    perform is-report-1-html-page-footer                         
    if sys-endprimaryfd = 0                                      
       perform is-report-1-html-page-header                      
    end-if.                                                      
