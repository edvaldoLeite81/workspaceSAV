is-report-1-setup-html-renderer.                                 
    set is-htmlrnd to iscobol-html-renderer:>"new".                
    call "c$fullname" using "report-1.html" ptr-dev-name.        
    is-htmlrnd:>"setPaperUrl" ( ptr-dev-name as string ).          
    is-htmlrnd:>"setPaperSize" ( "A4" as string ).                 
    is-htmlrnd:>"setLandscape" ( false as boolean ).               
    is-htmlrnd:>"setMargins" ( 0.0 as double 0.0 as double 0.0 as  
    double 0.0 as double ).                                      
    is-htmlrnd:>"setHeaderFooter" ( "" as string "" as string ).   
is-report-1-setup-print.                                         
    perform is-report-1-setup-html-renderer.                     
    if is-htmlrnd:>"setup" (is-paper-sizeint is-paper-margin)      
       compute is-paper-tmp = is-paper-width - is-margin-right - 
       is-margin-left                                            
       set is-report-1-page-width to is-htmlrnd:>"inchToPixel" (   
       is-paper-tmp as float )                                   
       compute is-paper-tmp = is-paper-height - is-margin-bottom 
       - is-margin-top                                           
       set is-report-1-page-height to is-htmlrnd:>"inchToPixel" (  
       is-paper-tmp as float )                                   
       perform is-report-1-print-tofile                          
       if valid-printf                                           
          perform is-report-1-do-print                           
          set is-report-1-page-width to 794                      
          set is-report-1-page-height to 1122                    
       end-if                                                    
    end-if.                                                      
is-report-1-preview.                                             
    perform is-report-1-print-tofile.                            
    if valid-printf                                              
       perform is-report-1-setup-html-renderer;;                 
       is-htmlrnd:>"setPreview" ( true as boolean )                
       is-htmlrnd:>"open"                                          
    end-if.                                                      
is-report-1-do-print.                                            
    is-htmlrnd:>"setPreview" ( false as boolean ).                 
    evaluate is-output-format                                    
    when "XLS"                                                   
       is-htmlrnd:>"exportXLS"                                     
    when "XLSX"                                                  
       is-htmlrnd:>"exportXLSX"                                    
    when "PDF"                                                   
       is-htmlrnd:>"exportPDF"                                     
    when other                                                   
       is-htmlrnd:>"open"                                          
    end-evaluate.                                                
is-report-1-print.                                               
    move spaces to is-output-format.                             
    perform is-report-1-print-para.                              
is-report-1-print-xls.                                           
    move "XLS" to is-output-format.                              
    perform is-report-1-print-para.                              
is-report-1-print-xlsx.                                          
    move "XLSX" to is-output-format.                             
    perform is-report-1-print-para.                              
is-report-1-print-pdf.                                           
    move "PDF" to is-output-format.                              
    perform is-report-1-print-para.                              
is-report-1-print-para.                                          
    perform is-report-1-print-tofile.                            
    if valid-printf                                              
       perform is-report-1-setup-html-renderer                   
       perform is-report-1-do-print                              
    end-if.                                                      
is-report-1-print-tofile.                                        
    move "report-1.html" to ptr-dev-name.                        
    move 1 to current-col                                        
    move 0 to is-report-1-cleftoffset                            
    move 0 to cnt-pixels                                         
    move 1 to ntopcolumnnum                                      
    perform is-printf-open-routine.                              
    if valid-printf                                              
       perform report-1-bef-print                                
       perform is-report-1-html-header                           
       perform is-report-1-html-page-header                      
       perform is-report-1-print-loop                            
       perform is-report-1-html-footer                           
       perform report-1-aft-print                                
       perform is-printf-close-routine                           
    end-if.                                                      
is-report-1-print-loop.                                          
    move 1 to tmp-printornot                                     
    move 0 to cnt-inpagepixels                                   
    move 1 to is-pagenum                                         
    move 1 to is-pagenum-sys                                     
    move 0 to sys-endprimaryfd                                   
    move 0 to report-footer-print-after-page                     
    perform is-report-1-master-print-loop                        
    if report-footer-print-after-page = 0                        
       move 1 to sys-endprimaryfd                                
       move is-report-1-page-height to cnt-inpagepixels          
       perform is-report-1-page-footer                           
    end-if.                                                      
